<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

	<groups>
		<run>
			<include name="smoke" />
		</run>
	</groups>

	<listeners>
		<listener
			class-name="AppiumFrameworkProject.src/main/java/utility.Listeners" />
	</listeners>
	
	<test thread-count="5" name="EndtoEndTC">
		<classes>
			<class name="androidTestCases.FillFormTestCase" />
		</classes>
	</test> <!-- Test -->


	<test thread-count="5" name="MultipleItamsToCart">
		<classes>
			<class name="androidTestCases.AddMultipleItemsToCart" />
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="errorValidation">
		<classes>
			<class name="androidTestCases.FillFormErrorValidation" />
		</classes>
	</test> <!-- Test -->

</suite> <!-- Suite -->
